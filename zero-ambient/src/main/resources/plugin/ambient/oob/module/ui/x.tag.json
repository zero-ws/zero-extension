[
    {
        "metadata": "name,标签名称"
    },
    {
        "metadata": "icon,图标,TAG",
        "$config": {
            "enabled": [
                "icon",
                "image"
            ]
        }
    },
    {
        "title": "是否启用",
        "dataIndex": "active",
        "sorter": true,
        "$render": "LOGICAL",
        "$mapping": {
            "true": "启用",
            "false": "禁用"
        },
        "$filter.config.dataType": "BOOLEAN",
        "$filter.config.items": [
            "true,启用",
            "false,禁用"
        ],
        "$filter.config.button": {
            "yes": "确认",
            "reset": "重置"
        },
        "$filter.config.width": {
            "radio": 110,
            "button": 55
        }
    },
    {
        "metadata": "description,描述,TEXT"
    },
    {
        "title": "创建人",
        "dataIndex": "createdBy",
        "$render": "USER",
        "$config": {
            "uri": "/api/user/:key",
            "field": "realname"
        },
        "$empty": "（系统）"
    },
    {
        "title": "创建时间",
        "dataIndex": "createdAt",
        "$render": "DATE",
        "$format": "YYYY-MM-DD"
    }
]